adjust.data <- function(df_){#
  	n <- nrow(df_)#
  	if(n<input$nsample){#
		df_[(n+1):input$nsample,1]<-0#
  		df_[(n+1):input$nsample,2]<-0#
  		row.names(df_)<-1:nrow(df_)#
  	}else{#
	  	df_<- df_[1:input$nsample,]#
  	}#
    df_$Positives<-as.numeric(df_$Positives)#
    df_$Total<-as.numeric(df_$Total)	#
    return(df_)#
}#
