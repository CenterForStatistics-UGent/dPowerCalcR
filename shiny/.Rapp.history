varBetween(c(0,0),c(0,0),1)
varBetween <- function(N,Npos,Vp){#
	VarTotal<-var(-log(1-Npos/N)/Vp)#
	NposA <- mean(Npos)#
	N <- mean(N)#
	pA<-1-NposA/N#
	VarPA<-pA*(1-pA)/N#
	vPoisson<-VarPA/(pA*Vp)^2#
	v<-VarTotal-vPoisson#
	if(is.na(v)){ v <- -1}#
	return(v)	#
}
