			alternative=paramsREL()[["alternative"]],#
			add=T,lty=3)#
	power.ddPCR.VB(delta=paramsREL()[["effect"]],#
			NNeg.fr=paramsREL()[["fraction"]],#
			NB=paramsREL()[["NB"]],#
			alpha=paramsREL()[["significance"]],#
			N=paramsREL()[["partitions"]],#
